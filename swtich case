public class MyClass {
  public static void main(String[] args) {
  int date=4;
  switch(date){
  case 1:
  System.out.println("monday");
  break;
  case 2:
  System.out.println("tuesday");
  break;
  case 3:
  System.out.println("wed");
  break;
  case 4:
  System.out.println("thursdayy");
  break;
  case 5:
  System.out.println("friday");
  break;
  }
  }
  }
  
